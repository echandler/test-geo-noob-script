{
   "action": {
      "default_popup": "src/settings.html",
      "default_title": "Temp GeoGuessr Timer"
   },
   "content_scripts": [ {
      "js": [ "src/play.js" ],
      "matches": [ "https://geoguessr.com/*", "https://www.geoguessr.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "This extension adds timers to GeoGuessr score bar",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "48": "img/icon-48.png"
   },
   "manifest_version": 3,
   "name": "Temp GeoGuessr timer",
   "options_ui": {
      "page": "src/settings.html"
   },
   "permissions": [ "storage" ],
   "version": "1.1.0"
}
